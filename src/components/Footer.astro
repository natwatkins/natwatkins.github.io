---
const { navLinks } = Astro.props;
import { showDeveloperText } from '../data';
---

<footer class='bg-gray-50 mt-10'>
  <div class='w-full max-w-screen-xl mx-auto p-4 md:py-8'>
    <div class='flex items-center mt-5 justify-between'>
      <ul
        class='flex w-full flex-wrap text-md md:text-lg justify-center items-center mb-6 font-medium'
      >
        {
          navLinks.map((link) => (
            <li>
              <a class='hover:underline me-4 md:me-6' href={`${link.href}`}>
                {link.name}
              </a>
            </li>
          ))
        }
      </ul>
    </div>
  </div>
</footer>
